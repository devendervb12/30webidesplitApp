<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="smax.batch30.A3Z30SPLITAPP01.controller.ProductList" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Product List">
		        <content>
		           <List items="{/ProductSet}" id="idList">
		             <items>
		                <StandardListItem title="{ProductID}" 
		                type="Active" press="onItemSelection" class="smaxcss"/>
		             </items>
		           </List>
		        </content>
		     </Page>
</mvc:View>